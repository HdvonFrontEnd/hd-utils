    "url-loader": "0.6.2",
    "vue": "2.5.21",
    "vue-loader": "^15.2.4",
    "vue-markdown-loader": "^2.4.1",
    "vue-router": "3.0.2",
    "vue-template-compiler": "2.5.21",
    "webpack": "4.29.6",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "3.2.1",
    "webpack-merge": "4.2.1",
